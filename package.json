{
  "name": "emuto",
  "version": "0.0.0-development",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest src",
    "build": "yarn run babel src/ -- -d lib/",
    "test:eslint": "yarn eslint src",
    "test:flow": "yarn flow check",
    "prepublish": "yarn run build",
    "checks":
      "yarn --ignore-engines && madge --circular src && yarn run test:flow && yarn run test:eslint --fix && jscpd --path src -b --limit 0",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.82.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9"
  },
  "dependencies": {
    "flow": "^0.2.3",
    "parsimmon": "^1.12.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/parsers/primitives/__tests__/tools.js",
      "<rootDir>/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kantord/emuto.git"
  }
}
